{
  "version": 2,
  "routes": [
    {
      "src": "/_next/static/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "dest": "static/$1"
    },
    {
      "src": "/static/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "dest": "static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/server.mjs"
    }
  ]
}