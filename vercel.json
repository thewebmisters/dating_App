{
  "version": 2,
  "public": "public",
  "builds": [
    {
      "src": "api/server.mjs",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/server.mjs"
    },
    {
      "src": "/assets/(.*)",
      "dest": "public/assets/$1"
    },
    {
      "src": "/(.*\\..*)",
      "dest": "public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "api/server.mjs"
    }
  ]
}